{
    "arrowParens": "always",
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "css",
    "bracketSameLine": true,
    "printWidth": 120,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "semi": false,
    "trailingComma": "es5",
    "plugins": ["prettier-plugin-tailwindcss"],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}
